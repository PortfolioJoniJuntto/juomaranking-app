{
  "name": "juomaranking",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios:local": "react-native run-ios --scheme 'JuomarankingLocal'",
    "ios:dev": "react-native run-ios --scheme 'JuomarankingDev'",
    "ios:prod": "react-native run-ios --scheme 'JuomarankingProd'",
    "start:prop": "react-native start",
    "start": "cross-env APP_ENV=dev npx react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "react-native clean-project-auto",
    "postinstall": "patch-package",
    "android:local": "react-native run-android --variant=localdebug --appId com.juomaranking.local",
    "android:local-release": "react-native run-android --variant=localrelease --appId com.juomaranking.local",
    "android:dev": "react-native run-android --variant=developmentdebug --appId com.juomaranking.development",
    "android:dev-release": "react-native run-android --variant=developmentrelease --appId com.juomaranking.development",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease"
  },
  "dependencies": {
    "@alessiocancian/react-native-actionsheet": "^3.1.2",
    "@gorhom/bottom-sheet": "^4.4.3",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-clipboard/clipboard": "^1.11.0",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@types/react-native-vector-icons": "^6.4.11",
    "dayjs": "^1.11.5",
    "deprecated-react-native-prop-types": "^2.3.0",
    "i18next": "^21.9.1",
    "intl": "^1.2.5",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "nativewind": "^2.0.0-next.65",
    "patch-package": "^6.4.7",
    "react": "18.2.0",
    "react-i18next": "^11.18.4",
    "react-native": "0.69.4",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^10.0.2",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-global-props": "^1.1.5",
    "react-native-localize": "^2.2.3",
    "react-native-permissions": "^3.6.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-star-rating-widget": "^1.2.0",
    "react-native-svg": "^13.0.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.2.0",
    "react-native-vision-camera": "^2.14.1",
    "realm": "^11.0.0-rc.1",
    "vision-camera-code-scanner": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/runtime": "^7.18.9",
    "@react-native-community/eslint-config": "^3.1.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/react-native": "^0.69.5",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "babel-jest": "^29.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "jest": "^29.0.0",
    "metro-react-native-babel-preset": "^0.72.1",
    "postinstall-postinstall": "^2.1.0",
    "react-native-clean-project": "^4.0.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
